{
    "rewrites": [
        { "source": "/src/(.*)", "destination": "/src/index.js" }
    ],
    "builds": [
        {
            "src": "./src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "./src/index.js"
        }
    ]
}
