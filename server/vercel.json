{   
    "builds":[
        {
            "src": "./src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "./src/index.js"
        }
    ],
    "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "https://stridedo.vercel.app" }
      ]
    }
  ]
}